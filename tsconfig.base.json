{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "exactOptionalPropertyTypes": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./build",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "target": "ES2022",
    "types": [],
    "useDefineForClassFields": true
  },
  "include": [],
  "ts-node": {
    "preferTsExts": true,
    "transpileOnly": true
  }
}
